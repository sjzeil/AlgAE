<ivysettings>
  <resolvers>
    <chain name="public" returnFirst="true">
      <url name="Forge350">
        <ivy pattern="http://forge350.cs.odu.edu/www/mavenrepo1/repo/[organization]/[module]/[revision]/ivy-[revision].xml" />
	<artifact pattern="http://forge350.cs.odu.edu/www/mavenrepo1/repo/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
	<artifact pattern="http://forge350.cs.odu.edu/www/mavenrepo1/repo/[artifact].[ext]" />
      </url>
      <ibiblio name="ibiblio" m2compatible="true" />
    </chain>

    <sftp name="Forge350Publish" host="forge350.cs.odu.edu">
        <ivy pattern="/var/lib/gforge/chroot/home/groups/mavenrepo1/htdocs/repo/[organization]/[module]/[revision]/ivy-[revision].xml" />
	<artifact pattern="/var/lib/gforge/chroot/home/groups/mavenrepo1/htdocs/repo/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
    </sftp>

  </resolvers>
</ivysettings>

<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <settings defaultResolver="default" />
  <credentials host="forge350.cs.odu.edu"
               username="deployer" passwd="deployer0"
               realm="Repository Archiva Managed internal Repository"/>
  <resolvers>
    <chain name="default">
      <ibiblio name="archiva-snapshot" m2compatible="true" root="http://forge350.cs.odu.edu/archiva/repository/snapshots" />
      <ibiblio name="archiva-internal" m2compatible="true" root="http://forge350.cs.odu.edu/archiva/repository/internal" />
    </chain>

    <url name="archiva-publish" m2compatible="true">
        <artifact pattern=
          "http://forge350.cs.odu.edu/archiva/repository/internal/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
        <ivy pattern="http://forge350.cs.odu.edu/archiva/repository/internal/[organisation]/[module]/[revision]/ivy-[revision].xml" />
    </url>
  </resolvers>
</ivy-settings>

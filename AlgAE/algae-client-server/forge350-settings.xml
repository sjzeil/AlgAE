<ivysettings>
  <resolvers>

    <chain name="public" returnFirst="true">
    
      <filesystem name="localFileResolver">
	     <artifact pattern="${user.home}/.ivy2/local-cache/[artifact]-[revision].[ext]"/>
      </filesystem>
      
      <url name="Forge350">
        <ivy pattern="http://forge350.cs.odu.edu/archive/[organization]/[module]/[revision]/ivy-[revision].xml" />
        <artifact pattern="http://forge350.cs.odu.edu/archive/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
        <artifact pattern="http://forge350.cs.odu.edu/archive/[artifact].[ext]" />
      </url>
      <ibiblio name="ibiblio" m2compatible="true" />
    </chain>

    <sftp name="publish" host="forge350.cs.odu.edu">
        <ivy pattern="/var/www/archive/[organization]/[module]/[revision]/ivy-[revision].xml" />
	    <artifact pattern="/var/www/archive/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
    </sftp>

  </resolvers>
</ivysettings>
